{
  "version": "1.0.0",
  "imports": {},
  "modules": {},
  "cache": {},
  "config": {
    "resolvers": {
      "prefixes": [
        {
          "prefix": "@local/",
          "resolver": "LOCAL",
          "type": "input",
          "priority": 20,
          "config": {
            "basePath": "./llm/modules"
          }
        }
      ]
